{
  "name": "jupyterlab_eduhelx_submission",
  "version": "0.1.0",
  "description": "A JupyterLab extension tfor submitting assignments in EduHeLx",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/helxplatform/jupyterlab_eduhelx_submission",
  "bugs": {
    "url": "https://github.com/helxplatform/jupyterlab_eduhelx_submission/issues"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Griffin Roupe",
    "email": "groupe@renci.org"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/helxplatform/jupyterlab_eduhelx_submission.git"
  },
  "workspaces": [
    "ui-tests"
  ],
  "scripts": {
    "build": "jlpm build:lib && jlpm build:labextension:dev",
    "build:prod": "jlpm clean && jlpm build:lib:prod && jlpm build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc --sourceMap",
    "build:lib:prod": "tsc",
    "clean": "jlpm clean:lib",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:lintcache": "rimraf .eslintcache .stylelintcache",
    "clean:labextension": "rimraf jupyterlab_eduhelx_submission/labextension jupyterlab_eduhelx_submission/_version.py",
    "clean:all": "jlpm clean:lib && jlpm clean:labextension && jlpm clean:lintcache",
    "eslint": "jlpm eslint:check --fix",
    "eslint:check": "eslint . --cache --ext .ts,.tsx",
    "install:extension": "jlpm build",
    "lint": "jlpm stylelint && jlpm prettier && jlpm eslint",
    "lint:check": "jlpm stylelint:check && jlpm prettier:check && jlpm eslint:check",
    "prettier": "jlpm prettier:base --write --list-different",
    "prettier:base": "prettier \"**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}\"",
    "prettier:check": "jlpm prettier:base --check",
    "stylelint": "jlpm stylelint:check --fix",
    "stylelint:check": "stylelint --cache \"style/**/*.css\"",
    "test": "jest --coverage",
    "watch": "run-p watch:src watch:labextension",
    "watch:src": "tsc -w --sourceMap",
    "watch:labextension": "jupyter labextension watch ."
  },
  "dependencies": {
    "@jupyterlab/application": "^3.6.5",
    "@jupyterlab/apputils": "^3.6.5",
    "@jupyterlab/coreutils": "^6.0.0",
    "@jupyterlab/filebrowser": "^3.6.5",
    "@jupyterlab/services": "^7.0.0",
    "@jupyterlab/ui-components": "^3.6.5",
    "@lumino/commands": "^1.21.1",
    "@lumino/coreutils": "^1.12.1",
    "@lumino/disposable": "^1.10.4",
    "@lumino/signaling": "^1.11.1",
    "@lumino/widgets": "^2.2.0",
    "@material-ui/core": "^4.8.2",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.54",
    "qs": "^6.11.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typestyle": "^2.4.0"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.6.5",
    "@jupyterlab/testutils": "^4.0.0",
    "@types/jest": "^29.2.0",
    "@types/json-schema": "^7.0.11",
    "@types/node": "^20.4.0",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.0.26",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.2.0",
    "mkdirp": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "source-map-loader": "^1.0.2",
    "style-loader": "^3.3.1",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recommended": "^8.0.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-prettier": "^2.0.0",
    "typescript": "~5.0.2",
    "yjs": "^13.5.0"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "styleModule": "style/index.js",
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "discovery": {
      "server": {
        "managers": [
          "pip"
        ],
        "base": {
          "name": "jupyterlab_eduhelx_submission"
        }
      }
    },
    "extension": true,
    "sharedPackages": {
      "@material-ui/core": {
        "singleton": true
      },
      "@material-ui/icons": {
        "singleton": true
      },
      "@material-ui/lab": {
        "singleton": true
      },
      "react": {
        "bundled": false,
        "singleton": true
      },
      "react-dom": {
        "bundled": false,
        "singleton": true
      }
    },
    "outputDir": "jupyterlab_eduhelx_submission/labextension"
  }
}
